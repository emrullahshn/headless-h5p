{
    "name": "alsay/headless-h5p",
    "description": "H5P integration for Laravel without any additional dependencies.",
    "type": "package",
    "require": {
        "php": ">=7.4",
        "bensampo/laravel-enum": ">=2.0",
        "laravel/framework": "^8|^9",
        "h5p/h5p-core": "^1|dev-master",
        "h5p/h5p-editor": "^1|dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "laravel/legacy-factories": "^1.0.4",
        "guzzlehttp/guzzle": "^7"
    },
    "autoload": {
        "psr-4": {
            "Alsay\\LaravelH5P\\": "src",
            "Alsay\\LaravelH5P\\Database\\Factories\\": "database/factories",
            "Alsay\\LaravelH5P\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alsay\\LaravelH5P\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Alsay\\LaravelH5P\\HeadlessH5PServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
